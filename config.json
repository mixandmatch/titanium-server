{
    "routes": [
        {
            "path": "/",
            "callback": "application#index"
        }, {
            "path": "/office",
            "method": "get",
            "callback": "place#list"
        }, {
            "path": "/canteen/byOffice",
            "method": "get",
            "callback": "canteen#byOffice"
        }, {
            "path": "/user/login",
            "method": "post",
            "callback": "user#login"
        }, {
            "path": "/user/register",
            "method": "post",
            "callback": "user#register"
        }, {
            "path": "/user/saveFeedback",
            "method": "post",
            "callback": "user#saveFeedback"
        },{
            "path": "/user/resetPwd",
            "method": "post",
            "callback": "user#resetPwd"
        }, {
            "path": "/user/search",
            "method": "get",
            "callback": "user#list"
        }, {
            "path": "/user/submitIssue",
            "method": "post",
            "callback": "user#submitIssue"
        }, {
            "path": "/event/",
            "method": "get",
            "callback": "event#list"
        }, {
            "path": "/event/byOffice",
            "method": "get",
